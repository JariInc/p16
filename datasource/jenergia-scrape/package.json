{
  "name": "jenergia-scrape",
  "version": "1.0.0",
  "description": "Jyväskylän Energia scraper",
  "main": "main.js",
  "scripts": {
    "start": "babel-node src/main.js",
    "prebuild": "rm -rf dist; mkdir dist",
    "build": "webpack --mode production",
    "create-stack": "npm run build && aws cloudformation package --template-file dist/aws-lambda.yaml --output-template-file ../../infra/lambda/jenergia-scrape.yaml --s3-bucket p16-lambda",
    "deploy": "aws cloudformation deploy --template-file  ../../infra/lambda/jenergia-scrape.yaml --stack-name p16-lambda-jenergia-scrape --capabilities CAPABILITY_IAM"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "dotenv": "^5.0.1",
    "influx": "^5.0.7",
    "lodash": "^4.17.5",
    "moment-timezone": "^0.5.14",
    "superagent": "^3.8.2",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.42",
    "@babel/core": "^7.0.0-beta.42",
    "@babel/node": "^7.0.0-beta.42",
    "@babel/polyfill": "^7.0.0-beta.42",
    "@babel/preset-env": "7.0.0-beta.42",
    "babel-loader": "^8.0.0-beta.0",
    "copy-webpack-plugin": "^4.5.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-node-externals": "^1.6.0"
  }
}
